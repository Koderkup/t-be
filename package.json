{
  "name": "tapply-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start-builded": "node dist/src/main.js",
    "start": "nest build && node dist/src/main.js",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "NODE_ENV=local nest start --watch",
    "start:dev-windows": "cross-env NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma-migrate": "prisma migrate dev --name init --skip-seed",
    "migrate-up": "npx prisma migrate deploy",
    "prisma-generate": "prisma generate",
    "seed-init": "npx @snaplet/seed init prisma/seed",
    "seed-sync": "npx @snaplet/seed --config prisma/seed/seed.config.ts sync",
    "seed": "npx tsx prisma/seed/seed.ts",
    "rerun": "npm run prisma-migrate && npm run seed-sync && npm run seed && npm run start:dev",
    "start:prod": "node dist/main",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose up -d"
  },
